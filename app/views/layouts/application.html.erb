<!DOCTYPE html>
<html>
  <head>
    <title>the birthdays</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <img class="ceiling_logo"><%= image_tag "logo.bmp" %></img>
    <table>
      <tbody>
        <tr>
          <td><%= link_to "Concerts", gigs_url %>         </td>      
          <td><%= link_to "Music", albums_url %>          </td>       
          <td><%= link_to "History", history_url %>       </td>    
          <td><%= link_to "Blog", blogs_url %>            </td>         
          <td><%= link_to "Contact", contact_us_url %>    </td> 
          <td>  <% if session[:user_id] %>
            <%= link_to 'Sign Out', session_url, :method => 'delete' %>
            <% else %>
            <%= link_to 'Sign Up', new_user_url %> or <%= link_to 'Sign In', new_session_url %>
            <% end %>
          </td>                                                       
          <td>  Partner Up!</td>                              
          <td><%= notice %></td>
        </tr>
      </tbody>
    </table>
  
  <%= yield %>
       
  </body>
</html>
