<h1>Edit <%= @album.title %> Detail</h1>

<%= flash[:album_not_updated] %>

<table>
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Release Date</th>
      <th scope="col">Cover Art</th>
    </tr>
  </thead>
  <tbody>
    
    <%= render "form" %>
    
    <tr>
      <td><%= link_to "Delete", destroy_album_url(@album.id), :method => :delete %></td>
    </tr>
  </tbody>
</table>

<p><%= link_to "New Album", new_album_url %></p>
<p><%= link_to "All Albums", albums_url %></p>
<p><%= link_to "Home", home_url %></p>