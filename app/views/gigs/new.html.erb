<h1>Post a Gig</h1>

<%= flash[:gig_not_created] %>

<p>Please fill in the following fields, and then submit:</p>
<table>
  <thead>
    <tr>
      <th scope="col">Date</th>
      <th scope="col">Venue</th>
      <th scope="col">Cover</th>
    </tr>
  </thead>
  <tbody>
    <%= render "form" %>
  </tbody>
</table>

<p><%= link_to "All Gigs", gigs_url %></p>
<p><%= link_to "Home", home_url %></p>