<h1><%= @gig.date %>, <%= @gig.venue %></h1>

<%= flash[:gig_created] %>
<%= flash[:gig_updated] %>

<table>
  <thead>
    <td>Date</td>
    <td>Venue</td>
    <td>Street</td>
    <td>City</td>
    <td>Zipcode</td>
    <td>Cover</td>
  </thead>
  <tbody>
    <tr>
      <td><%= @gig.date %></td>
      <td><%= @gig.venue %></td>
      <td><%= @gig.street %></td>
      <td><%= @gig.city %></td>
      <td><%= @gig.zipcode %></td>
      <td><%= @gig.ticket_price %></td>
    </tr>
  </tbody>
</table>

<p><%= link_to "Edit", edit_gig_url(@gig.id) %></p>
<p><%= link_to "Delete", destroy_gig_url(@gig.id), :method => :delete %></p>
<p><%= link_to "All Gigs", gigs_url %></p>