<h1>Edit <%= @user.name %>'s Profile</h1>

<%= flash[:user_not_updated] %>

<%= render "form", user:@user %>

<table>
  <tr>
    <td>
      <%= link_to "Delete", destroy_user_url(@user.id), 
            :confirm => "You sure 'bout that?",
            :method => :delete %>
    </td>
  </tr>
</table>

<p><%= link_to "Create Profile", new_user_url %></p>
<p><%= link_to "All Users", users_url %></p>
<p><%= link_to "Home", home_url %></p>